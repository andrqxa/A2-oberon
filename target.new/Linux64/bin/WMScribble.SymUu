MODULE WMScribble;
	IMPORT WMWindowManager, Strings, WMGraphics, WMRectangles, Modules, WMDialogs;
CONST 
TYPE 
	ScribbleWindow = OBJECT {EXCLUSIVE} (WMWindowManager.BufferWindow)
	VAR 
		lx, ly: SIZE; 

		PROCEDURE ^  & New*; 
		PROCEDURE ^ {OVERRIDE} PointerMove*(x, y: SIZE; keys: SET); 
		PROCEDURE ^ {OVERRIDE} PointerDown*(x, y: SIZE; keys: SET); 
		PROCEDURE ^ {OVERRIDE} KeyEvent*(ucs: SIZE; flags: SET; keySym: SIZE); 
	END ScribbleWindow; 
VAR 
	s: ScribbleWindow; 

	PROCEDURE ^ Open*; 
	PROCEDURE ^ Cleanup; 
BEGIN
END WMScribble.
